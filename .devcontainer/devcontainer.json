{
    "name": "DotNet Dev Container",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-dotnettools.csharp",
                "ms-azuretools.vscode-docker"
            ]
        }
    },
    "postCreateCommand": "dotnet restore",
    "remoteUser": "vscode",
    "forwardPorts": [
        5000,
        5001
    ]
}